<?xml version="1.0" encoding="utf-8"?>
<WorkflowBuilder Version="2.8.5"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xmlns:py="clr-namespace:Bonsai.Scripting.Python;assembly=Bonsai.Scripting.Python"
                 xmlns:rx="clr-namespace:Bonsai.Reactive;assembly=Bonsai.Core"
                 xmlns:scr="clr-namespace:Bonsai.Scripting.Expressions;assembly=Bonsai.Scripting.Expressions"
                 xmlns="https://bonsai-rx.org/2018/workflow">
  <Workflow>
    <Nodes>
      <Expression xsi:type="Combinator">
        <Combinator xsi:type="py:CreateRuntime">
          <py:PythonHome />
        </Combinator>
      </Expression>
      <Expression xsi:type="IncludeWorkflow" Path="Bonsai.ML.NeuralDecoder:ImportDecoderModule.bonsai" />
      <Expression xsi:type="IncludeWorkflow" Path="Bonsai.ML.NeuralDecoder:LoadClusterlessSpikeDecoder.bonsai">
        <Name>decoder</Name>
        <DecoderPath>../../../datasets/decoder_data/clusterless_spike_decoder.pkl</DecoderPath>
      </Expression>
      <Expression xsi:type="IncludeWorkflow" Path="Bonsai.ML.NeuralDecoder:LoadDecodedResultsFromFile.bonsai">
        <ResultsPath>../../../datasets/decoder_data/clusterless_spike_decoding_results.pkl</ResultsPath>
        <SamplingFrequency>PT0.1S</SamplingFrequency>
      </Expression>
      <Expression xsi:type="rx:BehaviorSubject">
        <Name>Data</Name>
      </Expression>
      <Expression xsi:type="SubscribeSubject">
        <Name>Data</Name>
      </Expression>
      <Expression xsi:type="MemberSelector">
        <Selector>Spikes</Selector>
      </Expression>
      <Expression xsi:type="IncludeWorkflow" Path="Bonsai.ML.NeuralDecoder:RunDecoder.bonsai">
        <Name>decoder</Name>
      </Expression>
      <Expression xsi:type="rx:BehaviorSubject">
        <Name>OnlineDecodedResults</Name>
      </Expression>
      <Expression xsi:type="GroupWorkflow">
        <Name>VisualizeDecoder</Name>
        <Workflow>
          <Nodes>
            <Expression xsi:type="SubscribeSubject">
              <Name>Data</Name>
            </Expression>
            <Expression xsi:type="MemberSelector">
              <Selector>Position</Selector>
            </Expression>
            <Expression xsi:type="scr:ExpressionTransform">
              <scr:Name>ConvertNaN</scr:Name>
              <scr:Expression>it.ToString() == "nan" ? double.NaN : Convert.ToDouble(it.ToString())</scr:Expression>
            </Expression>
            <Expression xsi:type="VisualizerMapping" />
            <Expression xsi:type="SubscribeSubject">
              <Name>OnlineDecodedResults</Name>
            </Expression>
            <Expression xsi:type="WorkflowOutput" />
          </Nodes>
          <Edges>
            <Edge From="0" To="1" Label="Source1" />
            <Edge From="1" To="2" Label="Source1" />
            <Edge From="2" To="3" Label="Source1" />
            <Edge From="3" To="4" Label="Source1" />
            <Edge From="4" To="5" Label="Source1" />
          </Edges>
        </Workflow>
      </Expression>
    </Nodes>
    <Edges>
      <Edge From="0" To="1" Label="Source1" />
      <Edge From="3" To="4" Label="Source1" />
      <Edge From="5" To="6" Label="Source1" />
      <Edge From="6" To="7" Label="Source1" />
      <Edge From="7" To="8" Label="Source1" />
    </Edges>
  </Workflow>
</WorkflowBuilder>